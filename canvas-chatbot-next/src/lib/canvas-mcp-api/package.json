{
  "name": "canvas-mcp-api",
  "version": "1.0.0",
  "description": "Enhanced Canvas API with MCP (Model Context Protocol) function calling capabilities",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "benchmark": "tsx src/benchmark.ts",
    "generate-docs": "swagger-jsdoc -d swaggerDef.js src/routes/*.ts -o openapi.json"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "axios": "^1.6.2",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "node-cache": "^5.1.2",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "@types/node": "^20.10.4",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@jest/globals": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "canvas",
    "lms",
    "mcp",
    "model-context-protocol",
    "api",
    "education",
    "function-calling"
  ],
  "author": "Canvas MCP Team",
  "license": "MIT"
}